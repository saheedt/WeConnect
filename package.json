  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development gulp build && gulp nodemon",
    "start": "node ./build/server.js",
    "postinstall": "gulp build",
    "test": "gulp test",
    "client:test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saheedt/WeConnect.git"
